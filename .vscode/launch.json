  "version": "0.2.0",
  "configurations": [

    {
      "name": "Launch Chrome against localhost",
      "request": "launch",
      "type": "chrome",
      "url": "http://localhost:4000",
      "webRoot": "${workspaceFolder}/nerfstudio/viewer/app"
    },
    {
      "name": "Python: Attach using Process ID",
      "type": "python",
      "request": "attach",
      "processId": "${command:pickProcess}",
      "justMyCode": true
    },
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": true
    },
    {
      "name": "Python: train",
      "type": "python",
      "request": "launch",
      "program": "nerfstudio/scripts/train.py",
      "console": "integratedTerminal",
      "args": [
        "instant_ngp",
        "--viewer.no-enable",
        "--logging.event-writer=wandb"
      ]
    },
    {
      "name": "Python: train ngp",
      "type": "python",
      "request": "launch",
      "program": "nerfstudio/scripts/train.py",
      "console": "integratedTerminal",
      "args": ["instant_ngp"]
    },
    {
      "name": "Python: train ngp nerfstudio",
      "type": "python",
      "request": "launch",
      "program": "nerfstudio/scripts/train.py",
      "console": "integratedTerminal",
      "args": [
        "instant_ngp",
        // "--experiment_name=nerfstudio_experiment",
        "--logging.local-writer.max-log-size=0",
        "--viewer.no-enable"
        // viewer
        // "--viewer.enable",
        // "--viewer.zmq-url=tcp://127.0.0.1:6001",
        // "--viewer.websocket-port=8014",
        // "--pipeline.model.randomize-background",
        // "--pipeline.model.cone-angle=0.00390625",
        // "nerfstudio-data"
      ]
    },
    {
      "name": "Python: train ngp-friends",
      "type": "python",
      "request": "launch",
      "program": "nerfstudio/scripts/train.py",
      "console": "integratedTerminal",
      "args": [
        "instant_ngp",
        "--viewer.enable",
        "--viewer.zmq-url=tcp://127.0.0.1:6001",
        "--viewer.websocket-port=8014",
        "--steps_per_save=1000000",
        "--steps_per_test=1000000",
        "--pipeline.model.density-field-params.base-scale=4",
        "--pipeline.model.density-field-params.num-cascades=1",
        "pipeline.datamanager.train-dataparser:friends-data-parser-config",
        "--pipeline.datamanager.train-dataparser.scene-scale=4"
      ]
    },
    {
      "name": "Python: train semantic_nerfw",
      "type": "python",
      "request": "launch",
      "program": "nerfstudio/scripts/train.py",
      "console": "integratedTerminal",
      "args": ["semantic_nerf"]
    },
    {
      "name": "Python: NeuS on Replica",
      "type": "python",
      "request": "launch",
      "program": "nerfstudio/scripts/train.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "env": { "CUDA_VISIBLE_DEVICES": "0" },
      "args": [
        "neus",
        "--pipeline.model.sdf-field.inside-outside=True",
        "--pipeline.model.mono-depth-loss-mult=0.1",
        "--pipeline.model.mono-normal-loss-mult=0.05",
        "--vis=wandb",
        "sdfstudio-data",
        "--data=data/sdfstudio-demo-data/replica-room0",
        "--include_mono_prior=True"
      ]
    },
    {
      "name": "Python: NeuS-facto on Replica",
      "type": "python",
      "request": "launch",
      "program": "nerfstudio/scripts/train.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "env": { "CUDA_VISIBLE_DEVICES": "0" },
      "args": [
        "neus-facto",
        "--pipeline.model.sdf-field.inside-outside=True",
        "--pipeline.model.mono-depth-loss-mult=0.1",
        "--pipeline.model.mono-normal-loss-mult=0.05",
        "--vis=wandb",
        "sdfstudio-data",
        "--data=data/sdfstudio-demo-data/replica-room0",
        "--include_mono_prior=True"
      ]
    },
    {
      "name": "Python: NeuS-facto on DTU (no mono-prior)",
      "type": "python",
      "request": "launch",
      "program": "nerfstudio/scripts/train.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "env": { "CUDA_VISIBLE_DEVICES": "0" },
      "args": [
        "neus-facto",
        "--pipeline.model.sdf-field.inside-outside=False",
        "--vis=wandb",
        "sdfstudio-data",
        "--data=data/sdfstudio-demo-data/dtu-scan65",
        "--include_mono_prior=False",
        "--auto-orient=True"
      ]
    },
    {
      "name": "Python: depth-nerfacto",
      "type": "python",
      "request": "launch",
      "program": "nerfstudio/scripts/train.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "env": { "CUDA_VISIBLE_DEVICES": "0" },
      "args": [
        "depth-nerfacto",
        "--data=/home/pierre.merriaux/data/nerf/output_pandaset_backAndFront_depth_028/transforms.json",
        // "--vis=wandb",
        "--pipeline.model.depth-loss-type=URF",
      ]
    },
    {
      "name": "Python: depth-nerfacto 2 cameras diff resolution",
      "type": "python",
      "request": "launch",
      "program": "nerfstudio/scripts/train.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "env": { "CUDA_VISIBLE_DEVICES": "0" },
      "args": [
        "depth-nerfacto",
        "--data=/home/pierre.merriaux/data/nerf/output_pandaset_backAndFront_depth_028/transforms_62img2cam.json",
        // "--vis=wandb",
        "--pipeline.model.depth-loss-type=URF",
        "--pipeline.datamanager.images-on-gpu=False",
        "--pipeline.datamanager.masks-on-gpu=False"

      ]
    },
    {
      "name": "Python: depth-nerfacto 5 cameras same resolution",
      "type": "python",
      "request": "launch",
      "program": "nerfstudio/scripts/train.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "env": { "CUDA_VISIBLE_DEVICES": "0" },
      "args": [
        "depth-nerfacto",
        "--data=/home/pierre.merriaux/data/nerf/output_pandaset_5front_depth_028/transforms.json",
        // "--vis=wandb",
        "--pipeline.model.depth-loss-type=URF",
      ]
    },
    {
      "name": "Python: depth-nerfacto + mask",
      "type": "python",
      "request": "launch",
      "program": "nerfstudio/scripts/train.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "env": { "CUDA_VISIBLE_DEVICES": "0" },
      "args": [
        "depth-nerfacto",
        "--data=/home/pierre.merriaux/data/nerf/output_pandaset_1front_depth_mask_011/transforms_depth_mask.json",
        // "--vis=wandb",
        "--pipeline.model.depth-loss-type=URF",
        "--pipeline.datamanager.train-num-rays-per-batch=1024",
        "--pipeline.datamanager.images-on-gpu=True", 
        "--pipeline.datamanager.masks-on-gpu=True"
      ]
    },
    {
      "name": "Python: depth-nerfacto-simplePixelSampler + mask",
      "type": "python",
      "request": "launch",
      "program": "nerfstudio/scripts/train.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "env": { "CUDA_VISIBLE_DEVICES": "0" },
      "args": [
        "depth-nerfacto-simplePixelSampler",
        "--data=/home/pierre.merriaux/data/nerf/output_pandaset_1front_depth_mask_011/transforms_depth_mask.json",
        // "--vis=wandb",
        "--pipeline.model.depth-loss-type=URF",
        "--pipeline.datamanager.train-num-rays-per-batch=4096",
        "--pipeline.datamanager.images-on-gpu=True", 
        "--pipeline.datamanager.masks-on-gpu=True",
        "--vis",
        "viewer+tensorboard",

      ]
    }
    ,
    {
      "name": "Python: nerfacto + mask without deph",
      "type": "python",
      "request": "launch",
      "program": "nerfstudio/scripts/train.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "env": { "CUDA_VISIBLE_DEVICES": "0" },
      "args": [
        "nerfacto",
        "--data=/home/pierre.merriaux/data/nerf/output_pandaset_1front_depth_mask_011/transforms_mask.json",
        // "--vis=wandb",
        //"--pipeline.model.depth-loss-type=URF",
        "--pipeline.datamanager.train-num-rays-per-batch=1024"
      ]
    }
    ,
    {
      "name": "Python: depth-nerfacto-simplePixelSampler + mask no center",
      "type": "python",
      "request": "launch",
      "program": "nerfstudio/scripts/train.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "env": { "CUDA_VISIBLE_DEVICES": "0" },
      "args": [
        "depth-nerfacto-simplePixelSampler",
        //"--data=/home/pierre.merriaux/data/nerf/output_pandaset_1front_depth_mask_011/transforms_depth_mask.json",
        "--data=/home/pierre.merriaux/data/nerf/output_pandaset_5front_object_static_centric_test_003/transforms.json",
        // "--vis=wandb",
        "--pipeline.model.depth-loss-type=URF",
        "--pipeline.datamanager.train-num-rays-per-batch=4096",
        "--pipeline.datamanager.images-on-gpu=True", 
        "--pipeline.datamanager.masks-on-gpu=True",
        "--pipeline.datamanager.dataparser.auto-scale-poses=False",
        "--pipeline.datamanager.dataparser.center-method=none",
        "--pipeline.datamanager.dataparser.scene-scale=2.0",
        "--vis",
        "viewer+tensorboard",

      ]
    }
  ]
}
